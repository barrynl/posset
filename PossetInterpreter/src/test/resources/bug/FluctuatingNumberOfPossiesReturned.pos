/* Once in a very while, this posset returns 50 instead of 190. Possibly this is caused 
 * by the order of the possets in the variable mapping. The code for this can be found 
 * in PossetFactoryImpl.reconnectCombinedPosset() . Note that 190 is the correct 
 * number of possies this posset should have, although it might seem a strange number.
 */
main
(
	Digit b[A];
	Digit d[B];
	Digit f[C];
	DigitEquals h
	(
		d1[A];
		d2[C];
		b[D];
	)
	DigitEquals m
	(
		d1[B];
		d2[C];
		b[E];
	)
	Or j
	(
		first[D];
		second[E];
		third[F];
	)
	True l[F];
)

DigitEquals
(
	Digit d1[A];
	Digit d2[B];
	Boolean b[G];
	Plus p
	(
		p1[C];
		p2[B];
		p3[A];
	)
	IsZero i
	(
		d1[C];
		b[G];
	)
)

IsZero
(
	Digit d1
	(
		d1[A];
		d2[B];
	)
	Boolean b
	(
		first[C];
	)
)
{
	(A,B,C)
	(#1,#1,#1)
	(#1,#2,#2)
	(#1,#3,#2)
	(#1,#4,#2)
	(#1,#5,#2)
	(#2,#1,#2)
	(#2,#2,#2)
	(#2,#3,#2)
	(#2,#4,#2)
	(#2,#5,#2)
}

Or
(
	Boolean first ( first[A]; )
	Boolean second ( first[B]; )
	Boolean third ( first[C]; )
)
{
	(A,B,C)
	(#1,#1,#1)
	(#1,#2,#1)
	(#2,#1,#1)
	(#2,#2,#2)
}

XOr
(
	Boolean first (first[A];)
	Boolean second (first[B];)
	Boolean third (first[C];)
)
{
	(A,B,C)
	(#1,#1,#2)
	(#1,#2,#1)
	(#2,#1,#1)
	(#2,#2,#2)
}

And
(
	Boolean first (first[A];)
	Boolean second (first[B];)
	Boolean third (first[C];)
)
{
	(A,B,C)
	(#1,#1,#1)
	(#1,#2,#2)
	(#2,#1,#2)
	(#2,#2,#2)
}

True
(
	Boolean first
	(
		first[A];
	)
)
{
	(A)
	(#1)
}

False
(
	Boolean first
	(
		first[A];
	)
)
{
	(A)
	(#2)
}

/* The boolean consists of two possible values. */
Boolean
(
	'2 first;
)

