/*
 * The plus relations for digits. All plus relations on multiple digits should be derivable from this
 * relation.
 */
Plus
(
	Digit p1
	(
		d1[A];
		d2[B];
	)
	Digit p2
	(
		d1[C];
		d2[D];
	)
	Digit p3
	(
		d1[E];
		d2[F];
	)
)
{
	(A,B,C,D,E,F)
	
	/* 0 + x = y */
	(#1,#1,#1,#1,#1,#1)
	(#1,#1,#1,#2,#1,#2)
	(#1,#1,#1,#3,#1,#3)
	(#1,#1,#1,#4,#1,#4)
	(#1,#1,#1,#5,#1,#5)
	(#1,#1,#2,#1,#2,#1)
	(#1,#1,#2,#2,#2,#2)
	(#1,#1,#2,#3,#2,#3)
	(#1,#1,#2,#4,#2,#4)
	(#1,#1,#2,#5,#2,#5)
	
	/* 1 + x = y */
	(#1,#2,#1,#1,#1,#2)
	(#1,#2,#1,#2,#1,#3)
	(#1,#2,#1,#3,#1,#4)
	(#1,#2,#1,#4,#1,#5)
	(#1,#2,#1,#5,#2,#1)
	(#1,#2,#2,#1,#2,#2)
	(#1,#2,#2,#2,#2,#3)
	(#1,#2,#2,#3,#2,#4)
	(#1,#2,#2,#4,#2,#5)
	(#1,#2,#2,#5,#1,#1)
	
	/* 2 + x = y */
	(#1,#3,#1,#1,#1,#3)
	(#1,#3,#1,#2,#1,#4)
	(#1,#3,#1,#3,#1,#5)
	(#1,#3,#1,#4,#2,#1)
	(#1,#3,#1,#5,#2,#2)
	(#1,#3,#2,#1,#2,#3)
	(#1,#3,#2,#2,#2,#4)
	(#1,#3,#2,#3,#2,#5)
	(#1,#3,#2,#4,#1,#1)
	(#1,#3,#2,#5,#1,#2)
	
	/* 3 + x = y */
	(#1,#4,#1,#1,#1,#4)
	(#1,#4,#1,#2,#1,#5)
	(#1,#4,#1,#3,#2,#1)
	(#1,#4,#1,#4,#2,#2)
	(#1,#4,#1,#5,#2,#3)
	(#1,#4,#2,#1,#2,#4)
	(#1,#4,#2,#2,#2,#5)
	(#1,#4,#2,#3,#1,#1)
	(#1,#4,#2,#4,#1,#2)
	(#1,#4,#2,#5,#1,#3)
	
	/* 4 + x = y */
	(#1,#5,#1,#1,#1,#5)
	(#1,#5,#1,#2,#2,#1)
	(#1,#5,#1,#3,#2,#2)
	(#1,#5,#1,#4,#2,#3)
	(#1,#5,#1,#5,#2,#4)
	(#1,#5,#2,#1,#2,#5)
	(#1,#5,#2,#2,#1,#1)
	(#1,#5,#2,#3,#1,#2)
	(#1,#5,#2,#4,#1,#3)
	(#1,#5,#2,#5,#1,#4)
	
	/* 5 + x = y */
	(#2,#1,#1,#1,#2,#1)
	(#2,#1,#1,#2,#2,#2)
	(#2,#1,#1,#3,#2,#3)
	(#2,#1,#1,#4,#2,#4)
	(#2,#1,#1,#5,#2,#5)
	(#2,#1,#2,#1,#1,#1)
	(#2,#1,#2,#2,#1,#2)
	(#2,#1,#2,#3,#1,#3)
	(#2,#1,#2,#4,#1,#4)
	(#2,#1,#2,#5,#1,#5)
	
	/* 6 + x = y */
	(#2,#2,#1,#1,#2,#2)
	(#2,#2,#1,#2,#2,#3)
	(#2,#2,#1,#3,#2,#4)
	(#2,#2,#1,#4,#2,#5)
	(#2,#2,#1,#5,#1,#1)
	(#2,#2,#2,#1,#1,#2)
